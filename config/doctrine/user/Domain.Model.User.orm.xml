<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Module\User\Domain\Model\User" table="client">
        <id name="id" type="integer" column="user_id">
            <generator strategy="NONE" />
        </id>
        <field name="username" type="string" column="username" />
        <field name="password" type="string" column="password" />
        <field name="createdAt" type="string" column="created_at" />
        <field name="email" type="string" column="email" unique="true" />
    </entity>
</doctrine-mapping>